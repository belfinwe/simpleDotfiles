#!/bin/bash
# Before use, add the following to "~/.bashrc".
# if [ -f ~/git/simpleDOtfiles/.alias ]; then
# . ~/git/simpleDotfiles/.alias
# fi
#
# Replace "~/git/simpleDOtfiles/.alias" with your filepath if needed.


# Custom aliases

# Navigation
alias cd.="cd .."
alias cdd="cd ~/Documents/"
alias cdg="cd ~/git/"

alias ll.="ll .."
alias lls="ls -alCF"

# Bash
alias bashre="source ~/.bashrc"

# Git
alias ggl="git pull"
alias ggs="git push"

# Other
alias katta="cat ~/git/simpleDotfiles/.alias"
alias kl="tty-clock -cs"

# Run dotfiles
alias runDot=". .programs"
